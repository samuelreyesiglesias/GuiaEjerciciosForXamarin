<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="GuiaEjercicios.Ejercicio4" Title="Ejercicio 4">
    <ContentPage.Content>
        <StackLayout>
            <Label Text="Ejercicio 4: App que identifique mayor de 3 numeros"/>
            <Entry x:Name="N1" Placeholder="Ingrese 1er numero"></Entry>
            <Entry x:Name="N2" Placeholder="Ingrese 2do numero"></Entry>
            <Entry x:Name="N3" Placeholder="Ingrese 2do numero"></Entry>

            <Button Text="Ejecutar" Clicked="Button_Clicked"></Button>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>